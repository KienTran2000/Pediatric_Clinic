@model List<PhongKhamNhi.Models.Entities.ChiNhanh>
@{
    ViewBag.Title = "Liên hệ";
}
<div class="page-banner overlay-dark bg-image" style="background-image: url(/Content/assets/img/bg_image_1.jpg);">
    <div class="banner-section">

    </div> <!-- .banner-section -->
</div> <!-- .page-banner -->

<div class="page-section">
    <div class="container">
        <h1 class="text-center wow fadeInUp">Danh sách chi nhánh</h1>

        @foreach (var item in Model)
        {
            <div>
                <span class="mai-location"></span>
                <span>@item.TenChiNhanh: @item.DiaChi</span>
        </div>
        }

    </div>
</div>

<div class="maps-container wow fadeInUp">
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3723.6570031286374!2d105.78418061476354!3d21.046405885988758!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3135ab2d88bb4195%3A0x3006e474cce20274!2zSOG7jWMgdmnhu4duIEvhu7kgdGh14bqtdCBRdcOibiBz4bux!5e0!3m2!1svi!2s!4v1642778665071!5m2!1svi!2s" width="100%" height="100%" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
</div>